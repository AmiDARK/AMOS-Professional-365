echo "***Re-Compiling Build Tools"
cd AMOS
APSystem/APCmp /c/Check_CLib.AMOS
APSystem/APCmp /c/Get_Chunk.AMOS
APSystem/APCmp /c/Library_Digest.AMOS
APSystem/APCmp /c/Make_Labels.AMOS
APSystem/APCmp /c/Make_Toktable.AMOS
cd /
